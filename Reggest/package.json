{
  "name": "reggest",
  "version": "1.0.0",
  "description": "Fitness planner recommender",
  "author": "Martin Dawson <reggest@info.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinDawson/Reggest.git"
  },
  "bugs": {
    "url": "https://github.com/MartinDawson/Reggest/issues"
  },
  "keywords": [
    "reggest",
    "question",
    "answer",
    "suggest",
    "faq",
    "fitness",
    "recommend",
    "planner",
    "plan"
  ],
  "homepage": "https://github.com/MartinDawson/Reggest",
  "license": "UNLICENSED",
  "scripts": {
    "prestart": "cross-env NODE_ENV=development webpack",
    "start": "npm-run-all --parallel development",
    "development": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "relay-compiler": "relay-compiler --src ./components --schema schema.graphql --extensions=js --extensions=jsx",
    "relay-compiler:watch": "npm run relay-compiler -- --watch",
    "get-schema": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql get-schema dev",
    "lint": "eslint --fix --ext .jsx,.js components",
    "clean": "rimraf ./wwwroot/build",
    "build-client": "cross-env NODE_ENV=production webpack",
    "build": "npm-run-all --sequential clean build-client"
  },
  "browserslist": [
    "> 3%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "edge >= 12",
    "Explorer 11"
  ],
  "devDependencies": {
    "aspnet-webpack": "^2.0.3",
    "css-modules-require-hook": "^4.0.6",
    "editorconfig": "^0.15.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-relay": "0.0.21",
    "graphql-cli": "^2.15.8",
    "jsdoc": "^3.5.5",
    "postcss": "6.0.0",
    "webpack-dev-server": "^2.1.0"
  },
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "dotenv": "^5.0.0",
    "es6-promise": "^4.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "farce": "^0.2.1",
    "file-loader": "^1.1.11",
    "found": "^0.3.4",
    "found-relay": "0.3.0-alpha.11",
    "graphql": "^0.12.3",
    "html-webpack-plugin": "^3.0.0",
    "less": "^2.3.1",
    "less-loader": "^4.0.4",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pluralize": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.8",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-helmet": "^5.2.0",
    "react-html-email-browser": "^3.0.1",
    "react-redux": "^4.3.0",
    "react-relay": "^1.5.0",
    "recompose": "^0.26.0",
    "redux": "^3.0.0",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0",
    "relay-compiler": "^1.5.0",
    "relay-compose": "^1.0.8",
    "relay-runtime": "1.5.0",
    "rimraf": "^2.6.2",
